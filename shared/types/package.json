{
  "name": "@msmebazaar/types",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for MSMEBazaar platform",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./api": "./api.ts",
    "./user": "./user.ts", 
    "./feature": "./feature.ts",
    "./payments": "./payments.ts",
    "./ml-services": "./ml-services.ts"
  },
  "files": [
    "*.ts",
    "*.d.ts",
    "*.js",
    "*.js.map",
    "*.d.ts.map"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf *.js *.d.ts *.js.map *.d.ts.map",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "keywords": [
    "types",
    "typescript",
    "msmebazaar",
    "shared"
  ],
  "author": "MSMEBazaar Team",
  "license": "MIT"
}
