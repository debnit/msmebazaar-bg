{
  "name": "msmebazaar-api-client",
  "version": "1.0.0",
  "description": "API Gateway service for MSMEBazaar platform, implemented in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "npm run clean && tsc -p tsconfig.json",
    "start": "node -r tsconfig-paths/register dist/src/index.js",
    "dev:build": "npm run build && npm run start",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "keywords": [
    "api",
    "gateway",
    "typescript",
    "msmebazaar",
    "node"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@msmebazaar/shared": "workspace:*",
    "bcryptjs": "^3.0.2",
    "cross-fetch": "^3.1.5",
    "dotenv-safe": "^9.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.9.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.3.1",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.3",
    "@typescript-eslint/parser": "^5.59.3",
    "eslint": "^8.39.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
